<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_numbers_form" model="ir.ui.view">
        <field name="name">view.numbers.form</field>
        <field name="model">numbers</field>
        <field name="arch" type="xml">
            <form string="Numbers">
                <sheet string="Logic Basic">
                    <group>
                        <group>
                            <field name="name_numbers"/>
                        </group>
                        <group>
                            <field name="floatt"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_numbers_tree" model="ir.ui.view">
        <field name="name">view.numbers.tree</field>
        <field name="model">numbers</field>
        <field name="arch" type="xml">
            <tree string="Numbers">
                <field name="name_numbers"/>
                <field name="floatt"/>
            </tree>
        </field>
    </record>    
</odoo>
